class Solution {
    public int[] twoSum(int[] nums, int target) {
       Map<Integer,Integer> mymap = new HashMap<>();
       for (int i =0;i<nums.length;i++){
           if (mymap.containsKey(target - nums[i])){
               return new int[]{mymap.get(target-nums[i]),i};
            }
            mymap.put(nums[i],i);
       }

       return new int[0];
    }
}
